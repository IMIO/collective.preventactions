<?xml version="1.0"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
    xmlns:i18n="http://xml.zope.org/namespaces/i18n">

     <object name="object_buttons" meta_type="CMF Action Category">
        <object name="enable_not_delete_object" meta_type="CMF Action" i18n:domain="cpskin">
            <property name="title" i18n:translate="">Object can not deleted</property>
            <property name="description"></property>
            <property name="url_expr">
                string:${context/absolute_url}/@@preventdelete_activation/enable_not_delete_object
            </property>
            <property name="link_target"></property>
            <property name="available_expr">
                context/@@preventdelete_activation/can_enable_not_delete_object
            </property>
            <property name="permissions">
                <permission>Manage Site</permission>
            </property>
            <property name="visible">True</property>
        </object>
        <object name="disable_not_delete_object" meta_type="CMF Action" i18n:domain="cpskin">
            <property name="title" i18n:translate="">Object can be deleted</property>
            <property name="description"></property>
            <property name="url_expr">
                string:${context/absolute_url}/@@preventdelete_activation/disable_not_delete_object
            </property>
            <property name="link_target"></property>
            <property name="available_expr">
                context/@@preventdelete_activation/can_disable_not_delete_object
            </property>
            <property name="permissions">
                <permission>Manage Site</permission>
            </property>
            <property name="visible">True</property>
        </object>

     </object>

</object>
